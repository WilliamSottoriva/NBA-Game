<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.scene.*?>	
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.*?> 
<?import javafx.geometry.*?>

<VBox  fx:controller="controller.AddTeamController" alignment="CENTER" styleClass="background" prefHeight="400.0" prefWidth="600.0" spacing="20" 
       xmlns:fx="http://javafx.com/fxml/1" >
    <GridPane fx:id="buttonGrid" alignment="center" hgap="50" style="-fx-padding: 0 0 20 0 ">
        <Label styleClass="label-h2" text="Team Details" GridPane.columnIndex="0" GridPane.rowIndex="0" GridPane.columnSpan="2" />
        <Label text="Name:" GridPane.columnIndex="0" GridPane.rowIndex="1"/>
        <TextField fx:id="nameTf" GridPane.columnIndex="1" GridPane.rowIndex="1"/>
    </GridPane>
    
    
    <Button fx:id="add" text="Add" onAction="#add" />
    
    
    

   
   
    <stylesheets>
        <URL value="@style.css"/>
    </stylesheets>
</VBox>

    

