<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.control.cell.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.*?>
<?import javafx.geometry.Insets?>  

<VBox fx:controller="controller.RecordController" styleClass="background"  alignment="CENTER" prefHeight="300" prefWidth="500" spacing="20" xmlns:fx="http://javafx.com/fxml/1" >
    
    <padding>
        <Insets top="0" bottom="10" left="0" right="0" />
    </padding>
    <Label styleClass="label-h2"  text="Season Record"/>
    <TableView fx:id="recordTv" style="-fx-padding: 0 0 20 0">
            <columns>
                <TableColumn fx:id="round" text="Round">

                </TableColumn>
                <TableColumn fx:id="game" text="Game">

                </TableColumn>
                <TableColumn fx:id="winTeam" text="Winning Team ">
                </TableColumn>
                <TableColumn fx:id="loseTeam" text="Losing Team">

                </TableColumn>
            </columns>
    </TableView>
    <Button fx:id="close" text="Close" onAction="#close"/>
    
    
    <stylesheets>
        <URL value="@style.css"/>
    </stylesheets>
</VBox>
